{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.6.0" } }],
          "react",
          "stage-2"
        ],
        "plugins": ["transform-async-to-generator", "transform-es2015-classes", "react-hot-loader/babel", "transform-es2015-destructuring", "transform-object-rest-spread", "styled-jsx/babel"],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.6.0" } }],
          "react",
          "stage-2"
        ],
        "plugins": ["transform-async-to-generator", "transform-es2015-classes", "transform-es2015-destructuring", "transform-object-rest-spread", "styled-jsx/babel"],
        "sourceMaps": "none"
      }
    }
  }
}
